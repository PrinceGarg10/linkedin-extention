{
  "name": "Linkedin Auto Request",
  "version": "0.0.2",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "128": "logo.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "tabs"
  ],
  "background":  {
    "service_worker": "background.js"
  },
   "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": ["content_script.js"]
    }
  ]
}